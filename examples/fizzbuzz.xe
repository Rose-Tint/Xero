import {xio}


void fizzbuzz(ubyte);


ubyte entry()
{
    fizzbuzz(1);
}


void fizzbuzz(ubyte num)
{
    if (num % 15 == 0)
    {
        print("FizzBuzz");
    }
    else if (num % 5 == 0)
    {
        print("Buzz");
    }
    else if (num % 3 == 0)
    {
        print("Fizz");
    }
    else
    {
        print(num);
    }

    if (num <= 100)
    {
        fizzbuzz(num + 1);
    }
}
